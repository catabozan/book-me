parameters:

    paths:
        - src

    # The level 9 is the highest level
    level: 9

    ignoreErrors:
        # - '#PHPDoc tag @var#'

    excludePaths:
        - ./src/App/Http/Middleware/
        - ./src/App/Providers

    parallel: 
        maximumNumberOfProcesses: 8

    checkMissingIterableValueType: true
